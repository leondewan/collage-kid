	<array>
		<string>armv7</string>
	</array>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleLightContent</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>NSCameraUsageDescription</key>
	<string>Collage Kid would like to use your camera so you can create your collage.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Collage Kid would like to use your microphone so that your collage can have sound.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Collage Kid will need to add your footage and photos to your camera roll.</string>
	<key>NSLocationUsageDescription</key>
	<string>You will need to provide your location in order to proceed.</string>
